<%= form_with url: sessions_path, local: true do |form|%>

  <%= show_alert %>

	<p>
    <%= form.label :email %><br>
    <%= form.text_field :email, id: :email %>
  </p>

  <p>
    <%= form.label :first_name %><br>
    <%= form.text_field :first_name, id: :first_name %>
  </p>

  <p>
    <%= form.label :last_name %><br>
    <%= form.text_field :last_name, id: :last_name %>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password, id: :password %>
  </p> 
  
  <p><%= form.submit 'Enter' %></p>
<% end %>

